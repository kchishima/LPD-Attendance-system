<div class="container">
  <div class="row">
    <div class="col-md-4">
      <h4>登録日</h4>
    </div>
     <div class="col-md-8">
     <h4><%= l @member.created_at.in_time_zone('Tokyo'), format: :original %></h4>
     </div>
  </div>
  <div class="row">
    <div class="col-md-4">
     <h4>id</h4>
    </div>
    <div class="col-md-8">
      <h4><%= @member.member_id %></h4>
    </div>
    </div>

  <div class="row">
    <div class="col-md-4">
      <h4>メールアドレス</h4>
    </div>
    <div class="col-md-8">
      <h4><%= @member.email %></h4>
    </div>
  </div>
    <div class="row">
      <div class="col-md-4">
       <h4>姓</h4>
      </div>
      <div class="col-md-8">
       <h4><%= @member.first_name %></h4>
      </div>
     <div class="col-md-4">
      <h4>名</h4>
    </div>
    <div class="col-md-8">
     <h4><%= @member.last_name %></h4>
    </div>
   </div>
  <div class="row">
    <div class="col-md-4">
      <h4>セイ</h4>
    </div>
    <div class="col-md-8">
     <h4><%= @member.first_name_kana %></h4>
    </div>
    <div class="col-md-4">
     <h4>メイ</h4>
    </div>
    <div class="col-md-8">
      <h4><%= @member.last_name_kana %></h4>
    </div>
  </div>
   <div class="row">
    <div class="col-md-4">
      <h4>住所</h4>
    </div>
    <div class="col-md-8">
     <h4><%= @member.address %></h4>
    </div>
  </div>
  <div class="row">
    <div class="col-md-4">
      <h4>電話番号</h4>
    </div>
    <div class="col-md-8">
      <h4><%= @member.phone_number %></h4>
    </div>

  </div>
   <%= link_to '編集', edit_admins_member_path(@member), method: :get %>
</div>

