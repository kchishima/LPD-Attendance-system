<div class="container">
  <h2>打刻一覧詳細ページ</h2>
  <div class="row mb-2">
    <div class="col-md-3">
      <%= "#{@member.first_name} #{@member.last_name}" %>
    </div>
    <div class="col-md-9">
      <%= @member.member_id %>
    </div>
  </div>
  <div id="calendar"></div>
</div>

<script>
  $('#calendar').fullCalendar({
    events: '/admins/attendance_informations.json?member_id=<%= params[:member_id] %>',
    timeFormat: 'HH:mm',
    displayEventEnd: true,
  });
</script>