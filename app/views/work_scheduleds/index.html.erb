 <div class="container">
    <h3>稼働予定日提出カレンダー</h3>
   <div id="calendar2"></div>
 </div>

 <script>

  // $('#calendar2').fullCalendar({
  //   events: '/work_scheduleds.json?member_id=<%= params[:member_id] %>',
  //   timeFormat: 'HH:mm',
  //   displayEventEnd: true,
  // });

  $('#calendar2').fullCalendar({
    // カレンダー上部を年月で表示させる
    titleFormat: 'YYYY年 M月',

    // 曜日を日本語表示
    dayNamesShort: ['日', '月', '火', '水', '木', '金', '土'],
    events: '/work_scheduleds.json?member_id=<%= params[:member_id] %>',
    timeFormat: 'HH:mm',
    displayEventEnd: true,
    selectable: true,
    navLinks: true,
    select: function(start, end, jsEvent, view){
      var date = start.format("YYYY/MM/DD")
      window.location = "/work_scheduleds/new" + "?date=" + date
    },
  });
</script>

